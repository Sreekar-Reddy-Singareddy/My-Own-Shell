#include<stdio.h>
#include<strings.h>
#include<stdlib.h>

int main () {
  FILE * file;
  char * input;
  char * paths[20];
  size_t size = 1024;
  input = (char *) malloc(255*sizeof(char));
  file = fopen("SAMPLE.txt", "r");
  // Get the current paths first
  int i=0;
  paths[i] = (char *) malloc(100*sizeof(char));
  getline(&paths[i], &size, file);
  while (paths[i] != NULL) {
    printf("Line %d: %s",i,paths[i]);
    i++;
    getline(&paths[i], &size, file);
  }
  //fgets(input, 255, (FILE *)file);
  //printf("Currently read this from file: %s", input);
  //input = (char *) malloc(255*sizeof(char));
  //getline(&input, &size, stdin);
  //fputs(input, file);
  fclose(file);
}
